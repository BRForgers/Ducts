{
  "schemaVersion": 1,
  "id": "ducts",
  "version": "${version}",

  "name": "Ducts",
  "description": "This mod add ducts.",
  "authors": [
    "BRForgers",
    "armelin1"
  ],
  "contact": {
    "homepage": "https://brforgers.com.br/",
    "sources": "https://github.com/BRForgers/Ducts"
  },

  "icon": "assets/ducts/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "br.com.brforgers.mods.ducts.Ducts"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "br.com.brforgers.mods.ducts.DuctsClient"
      }
    ]
  },
  "mixins": [
    "ducts.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.8.7",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
